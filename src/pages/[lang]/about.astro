---
import BaseAbout from "@/layouts/pages/About.astro";
import { languages } from "@/i18n/ui";

export const prerender = true;

export async function getStaticPaths() {
  return Object.keys(languages).map((l) => ({
    params: { lang: l },
  }));
}

const { lang } = Astro.params;
---

<BaseAbout lang={lang} />
